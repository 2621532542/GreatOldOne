	<property name="TakeDelay" value="15"/>
	<property name="DescriptionKey" value="机械加工台可以用来加工制造高级枪械和武器"/>
	<property name="EconomicValue" value="2000"/>
	<property name="Group" value="Building,advBuilding"/>
